  "scripts": {
    "build:web": "NODE_ENV=production webpack",
    "init": "npm install && npm run build:web",
    "dev:web": "NODE_ENV=development webpack-dev-server --devtool eval",
    "dev:server": "NODE_ENV=development nodemon src/server/index.js",
    "start": "concurrently \"npm run dev:web\" \"npm run dev:server\"",
    "lint": "eslint ./src/ --cache --fix",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
